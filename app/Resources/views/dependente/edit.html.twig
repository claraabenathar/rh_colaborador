{% extends 'base.html.twig' %}

{% block content %}
    <h1>Editar Dependente </h1>

    {{ form_start(edit_form) }}
        <div class="form-group">
            {{ form_row(edit_form.nome, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(edit_form.dataNascimento, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(edit_form.parentesco, {'attr': {'class': 'form-control'}}) }}
        </div>
        <button type="submit" class="btn btn-primary">Editar</button>
        <button type="button" onclick="javascript:history.go(-1)" class="btn btn-default">Voltar</button>
    {{ form_end(edit_form) }}


{% endblock %}
